{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
    "functions": {
        "api/generate-pdf.ts": {
            "memory": 3008,
            "maxDuration": 30
        }
    }
}